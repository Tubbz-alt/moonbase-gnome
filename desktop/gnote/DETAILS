          MODULE=gnote
           MAJOR=0.8
         VERSION=$MAJOR.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:a5244025633c031c106d30bb3902b2bb21fff5cf
        WEB_SITE=http://www.gnome.org/gnote
         ENTERED=20100509
         UPDATED=20120416
           SHORT="A C++ port of Tomboy"

cat << EOF
Gnote is a C++ port of Tomboy.
Upon first running Gnote, it will import Tomboy and eventually
StickyNote notes. These will be *copied*.
It is deliberate that it does not share the storage with Tomboy.
EOF
